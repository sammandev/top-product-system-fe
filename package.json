{
  "name": "ast-tools-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@sentry/vue": "^10.35.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/motion": "^3.0.3",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "dayjs": "^1.11.19",
    "echarts": "^6.0.0",
    "exceljs": "^4.4.0",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "jszip": "^3.10.1",
    "katex": "^0.16.27",
    "lodash-es": "^4.17.22",
    "npm-check-updates": "^19.3.1",
    "pinia": "^3.0.4",
    "pnpm": "^10.26.0",
    "vue": "^3.5.27",
    "vue-chartjs": "^5.3.3",
    "vue-echarts": "^8.0.1",
    "vue-router": "^4.6.4",
    "vuetify": "^3.11.6",
    "xlsx": "^0.18.5",
    "zod": "^4.3.5"
